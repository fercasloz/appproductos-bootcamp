{
  "name": "AppProductosProCode",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/http-client": "^4.2.0",
    "@sap-cloud-sdk/util": "^4.2.0",
    "@sap/cds": "^9",
    "@sap/low-code-event-handler": "^1.2.5",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^4.11.2",
    "express": "^4",
    "hdb": "^2.26.2",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/dev-cap-tools": "^1.31.0"
  },
  "scripts": {
    "start": "cds-serve",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open",
    "watch-appproductosprocode.productos": "cds watch --open appproductosprocode.productos/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-appproductosprocode.proveedores": "cds watch --open appproductosprocode.proveedores/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-appproductosprocode.clientes": "cds watch --open appproductosprocode.clientes/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-appproductosprocode.compras": "cds watch --open appproductosprocode.compras/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "private": true,
  "cds": {
    "requires": {
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "auth": {
        "[development]": {
          "kind": "dummy"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      }
    }
  },
  "lcap": true,
  "sapux": [
    "app/appproductosprocode.productos",
    "app/appproductosprocode.proveedores",
    "app/appproductosprocode.clientes",
    "app/appproductosprocode.compras"
  ]
}
